<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="673.0" prefWidth="479.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainTester">
   <children>
      <VBox layoutX="87.0" layoutY="40.0" prefHeight="106.0" prefWidth="306.0">
         <children>
            <Label text="City" />
            <TextField fx:id="txtCity" promptText="city">
               <VBox.margin>
                  <Insets left="15.0" />
               </VBox.margin>
            </TextField>
            <Label text="Country" />
            <TextField fx:id="txtCountry" promptText="country">
               <VBox.margin>
                  <Insets left="15.0" />
               </VBox.margin>
            </TextField>
         </children>
         <padding>
            <Insets bottom="15.0" />
         </padding>
      </VBox>
      <Button fx:id="btnSearch" layoutX="208.0" layoutY="159.0" mnemonicParsing="false" onAction="#switchToDetails" text="Search" />
   </children>
</AnchorPane>
